pipeline {
   agent any
   tools {
        maven 'Maven_3.6.0' 
    }
  
  stages
  { 
  	stage ('Compile Stage'){
   		steps {
   			
   				bat 'mvn clean compile'
   		}
   		
    }
   	
   	stage('Test Stage'){
   		steps{
   			
   			bat 'mvn test'
   		    
   		}
   	}
   	
   	stage('Deployment Stage'){
   		steps{
   			
   			bat 'mvn deploy'
   		    
   		}
   		
   	}
   	
   	
  }

}